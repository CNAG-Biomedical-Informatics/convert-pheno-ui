    "@testing-library/react": "^11.2.7",
    "@welldone-software/why-did-you-render": "^6.1.4",
    "babel-jest": "^27.0.2",
    "babel-loader": "^8.0.2",
    "babel-plugin-istanbul": "^6.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.2.0",
    "csv-generator": "^1.0.4",
    "cypress": "^9.7.0",
    "cypress-failed-log": "^2.9.2",
    "cypress-file-upload": "^5.0.8",
    "cypress-keycloak-commands": "^1.2.0",
    "cypress-log-to-output": "^1.1.2",
    "cypress-multi-reporters": "^1.5.0",
    "cypress-parallel": "^0.9.0",
    "docz": "^2.3.1",
    "file-loader": "^6.1.1",
    "html-webpack-plugin": "^4.5.2",
    "husky": "^7.0.2",
    "istanbul-badges-readme": "^1.4.0",
    "jest": "^27.0.4",
    "jest-sonar-reporter": "^2.0.0",
    "jsdoc": "^3.6.7",
    "junit-report-merger": "^3.0.2",
    "kill-port": "^2.0.1",
    "lint-staged": "^11.2.3",
    "mocha-junit-reporter": "^2.0.2",
    "mochawesome": "^6.3.1",
    "mochawesome-merge": "^4.2.0",
    "npm-groovy-lint": "^9.0.0",
    "react-styleguidist": "^11.1.7",
    "sonarqube-scanner": "^2.8.1",
    "style-loader": "^0.23.1",
    "webpack": "^4.19.1",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "3.11.2",
    "webpack-merge": "^5.8.0"
  }
}
