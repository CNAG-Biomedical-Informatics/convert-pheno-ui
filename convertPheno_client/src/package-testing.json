  },
  "jestSonar": {
    "reportPath": "testing/jest/output",
    "reportFile": "jest-sonar-report.xml",
    "indent": 4
  },
  "scripts": {
    "cov": "webpack serve --config webpack.cov.js",
    "build": "webpack --config webpack.prod.js",
    "dev": "webpack serve --open --config webpack.dev.js --open firefox",
    "prod": "webpack serve --config webpack.dev.js",
    "dev:docker": "webpack serve --config webpack.dev.js",
    "dev:docker:testing": "webpack serve --config webpack.cy.js",
    "stop": "npx kill-port 4000",
    "webpack": "webpack serve --open --config webpack.dev.js",
    "webpack:test": "webpack serve --config webpack.cy.js",
    "lint": "eslint src",
    "lint:fix": "npm run lint -- --fix",
    "pretest": "bash getKCTokens.sh && bash cypress/utils/deleteReports.sh && webpack serve --open --config webpack.cy.js&",
    "test": "cypress run --config-file cypress/cfg.json",
    "test:components": "cypress run-ct --config-file cypress/cfg.json && jest --config testing/jest/jest.config.js",
    "posttest": "bash cypress/utils/mergeReports.sh",
    "jest": "jest --config testing/jest/jest.config.js",
    "jest:make-badges": "istanbul-badges-readme --coverageDir='./testing/jest/coverage/'",
    "precy": "bash getKCTokens.sh && bash cypress/utils/deleteReports.sh && webpack serve --open --config webpack.cy.js&",
    "cy": "cypress open --config-file cypress/cfg.json",
    "cy:run": "npm run precy && cypress run --config-file cypress/cfg.json",
    "cy:run:jenkins": "docker exec cy-docker cypress run --config-file /cy/cypress/cfg.json",
    "cy:run:parallel": "cypress run --config-file cypress/cfg.json",
    "cy:parallel": "npm run precy && cypress-parallel -v -s cy:run:parallel -t 4 -d cypress/integration/",
    "cy:run:one": "npm run precy && cypress run --config-file cypress/cfg.json --spec cypress/integration/submissionTemplates.spec.js",
    "cy:open-ct": "npm run precy && cypress open-ct --config-file testing/cypress/cfg.json",
    "cy:run-ct": "cypress run-ct --config-file cypress/cfg.json",
    "cy:make-badges": "istanbul-badges-readme --coverageDir='./cypress/coverage/'",
    "cy:merge:mochawesome": "mochawesome-merge cypress/reports/mocha/*.json > cypress/reports/mocha_merged.json",
    "cy:generate_mochawesome_report": "marge cypress/reports/mocha_merged.json --reportDir testing/output/. --inline",
    "merge-cov": "node ./testing/utils/mergeCoverage.js",
    "make-badges": "istanbul-badges-readme --coverageDir='./testing/output/coverage/'",
    "docs": "jsdoc -c jsdoc.conf.json",
    "sonar": "node sonarqube-scanner.js",
    "jenkins:lint": "npm-groovy-lint --fix --verbose -f '**/Jenkinsfile' -i '**/example/Jenkinsfile'",
    "jenkins:format": "npm-groovy-lint --format --verbose -f '**/Jenkinsfile' -i '**/example/Jenkinsfile'",
    "precompose:run": "bash getKCTokens.sh && docker-compose -f Docker-compose-testing.yaml up -d",
    "precompose:build": "docker-compose -f Docker-compose-testing.yaml up -d --build",
    "compose:run": "docker exec 3tr-client_vnc_1 cypress run --config-file /cy/cypress/cfg.json",
    "compose:open": "bash compose.sh",
    "compose:view": "bash compose.sh & sleep 10 && flatpak run org.gnome.Connections",
    "compose:view:browser": "bash compose.sh & sleep 5 && novnc --listen 6081 --vnc localhost:5920",
    "compose:view:browser:stop": "npx kill-port 6081",
    "compose:parallel:script": "cypress run --config-file /cy/cypress/cfg.json",
    "compose:parallel:run": "cypress-parallel -v -s compose:parallel:script -t 4",
    "compose:parallel": "npm run precompose:run && docker exec 3tr-client_vnc_1 npm run compose:parallel:run",
    "predocker": "bash getKCTokens.sh && bash cyDocker.sh",
    "docker:run": "DISPLAY=:20 CYPRESS_RUN_BINARY=/root/.cache/Cypress/8.3.0/Cypress/Cypress cypress run --config-file /cy/cypress/cfg.json",
    "docker:parallel": "cypress-parallel -v -s docker:run -t 4",
    "docker": "docker exec -it cy-docker npm run docker:parallel",
    "docker:jenkins": "docker run -d --network=host --name cy-docker -e NO_COLOR=1 -v $PWD:/cy -w /cy --entrypoint sleep rdregistry1.rd-connect.eu:5000/cypress:8.3.0-included-nonroot infinity",
    "docker:jenkins:run": "CYPRESS_RUN_BINARY=/root/.cache/Cypress/8.3.0/Cypress/Cypress cypress run --config-file /cy/cypress/cfg.json",
    "docker:jenkins:parallel": "cypress-parallel -v -s docker:jenkins:run -t 3",
    "jenkins": "docker exec cy-docker npm run docker:jenkins:parallel",
    "semgrep": "semgrep --config auto src"
  },
  "nyc": {
    "report-dir": "cypress/coverage",
    "reporter": [
      "cobertura",
      "json-summary",
      "json",
      "lcov"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@babel/plugin-transform-runtime": "^7.14.3",
